.loginbox
{
	width: 270px;
	height:147px;
	float:right;
	margin-right:45px;
	margin-top:15px;
	padding-top:5px;
	padding-right:15px;
	padding-left:15px;
	padding-bottom: 5px;

	clear:both;
	/* background-image:url(http://f.canadapredicts.ca/images/bg-loginbox.png); */

	background-image: linear-gradient(bottom, rgb(225,223,223) 15%, rgb(255,255,255) 58%);
	background-image: -o-linear-gradient(bottom, rgb(225,223,223) 15%, rgb(255,255,255) 58%);
	background-image: -moz-linear-gradient(bottom, rgb(225,223,223) 15%, rgb(255,255,255) 58%);
	background-image: -webkit-linear-gradient(bottom, rgb(225,223,223) 15%, rgb(255,255,255) 58%);
	background-image: -ms-linear-gradient(bottom, rgb(225,223,223) 15%, rgb(255,255,255) 58%);
	
	background-image: -webkit-gradient(
		linear,
		left bottom,
		left top,
		color-stop(0.15, rgb(225,223,223)),
		color-stop(0.58, rgb(255,255,255))
	
	);
	
	-moz-box-shadow:1px 1px 2px 0px #888888;
	-webkit-box-shadow:1px 1px 2px 0px #888888;
	box-shadow:1px 1px 2px 0px #888888;

	border: 1px solid #bbbbbb;
	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
	border-radius: 5px;
}

div.profilebox
{
	width: 300px;
	height:140px;
	float:right;
	margin-right:45px;
	margin-top:15px;
	text-align: left;

	clear:both;

	background-image: linear-gradient(bottom, rgb(225,223,223) 15%, rgb(255,255,255) 58%);
	background-image: -o-linear-gradient(bottom, rgb(225,223,223) 15%, rgb(255,255,255) 58%);
	background-image: -moz-linear-gradient(bottom, rgb(225,223,223) 15%, rgb(255,255,255) 58%);
	background-image: -webkit-linear-gradient(bottom, rgb(225,223,223) 15%, rgb(255,255,255) 58%);
	background-image: -ms-linear-gradient(bottom, rgb(225,223,223) 15%, rgb(255,255,255) 58%);
	
	background-image: -webkit-gradient(
		linear,
		left bottom,
		left top,
		color-stop(0.15, rgb(225,223,223)),
		color-stop(0.58, rgb(255,255,255))
	
	);
	
	-moz-box-shadow:1px 1px 2px 0px #888888;
	-webkit-box-shadow:1px 1px 2px 0px #888888;
	box-shadow:1px 1px 2px 0px #888888;

	border: 1px solid #bbbbbb;
	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
	border-radius: 5px;
}

input
{
    font-family: Verdana, Geneva, sans-serif;
    font-size:18px;
    background: #ffffff;
    border: 1px solid #bbbbbb;
    border-radius:6px;
	display:inline-block;
	padding:3px;
}

input:hover, textarea:hover,  
input:focus, textarea:focus
{
	border-color: #C9C9C9;  
}

input[type="submit"]
{
	cursor: pointer;
}

input.rounded
{
	border: 1px solid #bbbbbb;

	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
	border-radius: 5px;

	font-size: 16px;
	padding: 4px 7px;
	outline: 0;
	margin-top: -7px;
	-webkit-appearance: none;

	-moz-box-shadow:	0px 1px 1px 0px #888888 inset;
	-webkit-box-shadow:	0px 1px 1px 0px #888888 inset;
	box-shadow:		0px 1px 1px 0px #888888 inset;
}

div.loginbox input.rounded
{
	width: 250px;
}

div.loginbox input.rounded[type=checkbox]
{
	width: auto;
}

input.rounded:focus {
	border-color: #bbbbbb;
}

